$color_p = #000
$color_s = #212121
$color_t = #3d3d3d
$color_f = #797979
$color_s = #e3bb56
$color_brow = #e3bb56
$color_alt = #d7ab9e
$color_title = #000
$color_gray_3 = #797979
$color_boton_primario = #000
$color_boton_primario_hover = #333
$fonttext = 'Arial'
$fonttext_f1 ='OpenSans-Bold'
$fonttext_f2 ='OpenSans-Semibold'
$fonttext_f3 ='OpenSans'



/**********************/
/**HEADER**/
.dom-content-header
	z-index 13
	padding 0px
	background #29b34b
	//background $color_p
	//background-image url(../../images/common/back-header.jpg)
	min-height auto
	display inline-block
	width 100%
	border-bottom 1px solid #ddd
	margin 0 0 0px 0
	position fixed
	.navbar-nav
		float none
	.navbar-nav > li
		float none
		display inline-block
	.navbar-nav>li>a
		margin 0px
		padding 5px 15px 5px
	.nav>li>a:hover, .nav>li>a:focus
		background none
	.navbar-header
		display none
	.top_slogan
		background #fff
		display inline-block
		margin 0
		width 100%
		padding 3px 0
		.promotion
			display block
			width 100%
			text-align center
			font-size 13px
			margin 0px
			clear both
			font-weight normal
			font-family $fonttext_f3
			padding 0px 0
			text-transform none
			span
				padding 0px
				color #ffffff

		.client_pref
			margin-right 15px
			padding-right 15px
			display inline-block
			margin-top 0px
			color #ffffff
			font-size 12px
			border-right 1px solid #fff

			span
				margin 0 0 0 8px
			i
				color #fff
				margin 1px 6px 0 0
			//	float left
			// a
			// 	color #f1f1f1
			// 	font-size 11px
			// 	font-weight normal
			// 	text-decoration none
			// 	font-family $fonttext_f2
			// 	float left
			// 	margin-top 2px
			// 	&:hover
			// 		text-decoration underline

	.login
		font-family $fonttext_f2
		//margin-top 15px
		display inline-block
		//margin-right 0px
		float right


		a
			text-decoration none
			color #29b34b
			font-size: 11px
			float left

			&:hover
				color #29b34b
			span
				float right
				display block


			i
				float right
				display none
			u
				float right
				display block

		.logout
			margin 0 0 0 10px
			color: #ccc
			font-size: 11px
			font-weight: normal
			float right
			a
				text-decoration none
				float none
				&:hover
					color #29B34B
			b
				display none
		.glyphicon-user
			vertical-align: middle !important;
			border-radius: 50%;
			padding 0px;
			font-size 22px
			color #29B34B
			margin-top 17px

	#logo
		display block
		text-align center
		margin-right -15px


		.visible_descktop
			display block
		.visible_mobile
			display none
		img
			display inline-block
			//margin-right -70px
			//width auto !important
			//height auto !important

	.img_head
		//display block
		//margin 5px 0 0 19px
		.visible_descktop
			display block
		.visible_mobile
			display none
		img
			// width auto !important
			// height auto !important
			display inline-block

	.menumyaccount
		margin 33px 0 0 0
		display inline-block
		border-right 1px solid #ffffff
		width auto
		a
			color #fff
			text-decoration none
			font-size 11px
			text-align left
			line-height 12px
			display inline-block
			.ico-user
				background url(../../images/common/sprite.png) no-repeat -8px -5px
				width 19px
				height 20px
				float left
				margin 2px 10px 0 0
			span
				float left
				width 40px
				margin 0 20px 0 0
		b
			border-left 1px solid #fff
			color #fff
			display inline-block
			font-size 11px
			font-weight normal
			line-height 14px
			padding-left 25px
			text-align left
			text-decoration none
			width 80px
			span
				display block
			b
				display none

	.menushopcart
		margin 5px 0px 0 0
		height auto
		display block
		width auto
		float none
		/*margin-top 16px
		margin-right 12px*/
		i
			float none
			margin-right 5px
			font-size 22px
			color #fff
			margin-top 2px
			display inline-block
		a
			color #e3bb56 !important
			margin 0px 0 0 2px
			display block
			font-size 24px
			line-height 25px
			font-family $fonttext_f3
			letter-spacing -3px
			text-align center
			float none
			display inline-block


		.ico-cart
			width 26px
			height 20px
			float left
			margin 3px 0px 0 0

	.header_sec_search
		display inline-block
		height auto
		width auto
		margin 5px 0 0 0px
		.search_wrapper
			width 350px
			background #fff
			z-index 3000
			position absolute
			top -3px
			right 0px
			/*margin-top -7px
			margin-right -13px*/


			.search_box
				border-right 1px solid #29B34B
				float left
				height 36px
				width 87%
				.input_search
					width 86%
					right 0px
					height 25px
					border none
					padding-left 13px
					color $color_s
					border none
					font-weight 100
					border-radius 0px
					margin 3px 0 0 0
					font-size 16px
					line-height 18px
					float left
					text-align left
					font-family $fonttext_f1
					outline 0px
				.button
					cursor pointer
					border none
					font-size 22px
					border-radius 0
					float right
					text-align center
					background transparent
					color $color_boton_primario
					padding 5px 13px 0 0
					&:hover
						//background: $color_t

			.close_popup
				width 13%
				color #29B34B
				font-size 25px
				cursor pointer
				display inline-block
				padding 0 8px 0 8px
				text-align center

		.t_search
			.fa-search
				// background url(../../images/common/search.jpg) no-repeat 0 0
				// background-size 90%
				font-size 22px
				color #fff
	.visible_search_mobile
		display none
	.menu_secondary
			display block
			ul
				li
					z-index 910
			.nav>li>a:hover, .nav>li>a:focus
				background none
	/**MENU LISTA*/
	.dropdown-menu > li > a
		text-align left
	.dropdown-menu > li > a:hover, .dropdown-menu > li > a:focus
		background #000

	/**MENU PANEL*/
	.menuhome
		display block
		border none
		position initial
		.navbar-nav
			li
				padding-top 7px
				padding-bottom 5px
				a
					margin 0
		.navbar-brand
			display none
		#nav_menu > li
			// padding 10px 15px
			// float left
			position inherit

		#nav_menu > li > a
			font-size 15px
			text-transform uppercase
			color #fff
			font-family $fonttext_f2
			text-decoration none
			background-color transparent

			&:hover
				background transparent
				color $color_brow
		.dropdown-menu
			padding-bottom 10px
			li
				padding 5px 0
				a
					&:hover
						background transparent
						color $color_gray_3

		/******************INICIO MENU*****************************************/
		/* MENU OVER */
		.navbar-nav > li > .dropdown-menu
			margin-top 2px
			padding 0px
			height auto
			//display block

		.dropdown
			position static

			.dropdown-menu
				min-width:100%;
				width:100%;
				left:0;
				top:auto;
				background-image:url(../../images/common/back_header_menu.jpg);
				position:absolute;
				position: -ms-page;
				box-shadow:none;
				border-radius:0;
				border-bottom 1px solid #ddd
				border-top 0px
				border-left 0px
				border-right 0px
				ol
					padding 0
					margin 0
					li
						width auto
						float left
						margin 8px 0 0px 0px
						background transparent
						font-size 14px
						font-family $fonttext_02
						padding 0px 5% 0 0
						text-transform none
						color $color_s
						list-style none

				strong
					width auto
					float left
					margin 0px 0 0px 0px
					background transparent
					font-size 12px
					font-family $fonttext_01
					padding 0px
					text-transform capitalize
					a
						color $color_p
						&:hover
							color $color_p
				li
					width 100%
					float left
					margin 2px 0 0px 0px
					background transparent
					font-size 12px
					font-family $fonttext_01
					padding 0px
					text-transform capitalize
					a
						color $color_s
						padding 3px 0px
						text-decoration none
						&:hover
							color $color_p
							text-decoration underline

				span
					border-right 1px solid #00e628
					float left
					margin 11px 15px
					width 1px
					height 18px

				.menu-full
					padding 10px 0 12px 0

			.dropdown-menu > li > a:hover, .dropdown-menu > li > a:focus
				background-color transparent
				color $color_s

		.dropdown-men-i9
				min-width:100%;
				width:100%;
				left:0;
				top:auto;
				background-image:url(../../images/common/back_header_menu.jpg);
				position:absolute;
				position: -ms-page;
				box-shadow:none;
				border-radius:0;
				border:none;
				display block
				ul
					list-style none
				li
					list-style none
					width auto
					float left
					margin 8px 0 0px 0px
					background transparent
					font-size 17px
					font-family $fonttext_f1
					padding 0px
					text-transform uppercase
					a
						color #212121
						padding 3px 0px
						text-decoration none

				span
					border-right 1px solid #00e628
					float left
					margin 11px 15px
					width 1px
					height 18px

		.navbar-default .navbar-nav > .open > a, .navbar-default .navbar-nav > .open > a:hover, .navbar-default .navbar-nav > .open > a:focus
			background-color transparent
			color #555

		#nav_menu, #nav_menu ul
			list-style: outside none none;
			padding 0px

		#nav_menu
			float: left;
			position: initial;
			width: 100%;
			z-index: 900;




