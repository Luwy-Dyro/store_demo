$color_p = #000
$color_s = #212121
$color_t = #3d3d3d
$color_f = #797979
$color_s = #e3bb56
$color_brow = #e3bb56
$color_alt = #d7ab9e
$color_title = #000
$color_gray_3 = #797979
$color_boton_primario = #000
$color_boton_primario_hover = #333
$fonttext = 'Arial'
$fonttext_f1 ='OpenSans-Bold'
$fonttext_f2 ='OpenSans-Semibold'
$fonttext_f3 ='OpenSans'



/**********************/
/**HEADER**/
.dom-content-header
	z-index 13
	padding 0px
	background #29b34b
	//background $color_p
	//background-image url(../../images/common/back-header.jpg)
	min-height auto
	display inline-block
	width 100%
	border-bottom 1px solid #ddd
	margin 0 0 0px 0
	position fixed

	.top_slogan
		background #fff
		display inline-block
		margin 0
		width 100%
		padding 3px 0
		.promotion
			display block
			width 100%
			text-align center
			font-size 13px
			margin 0px
			clear both
			font-weight normal
			font-family $fonttext_f3
			padding 0px 0
			text-transform none
			span
				padding 0px
				color #ffffff

		.client_pref
			margin-right 15px
			padding-right 15px
			display inline-block
			margin-top 0px
			color #ffffff
			font-size 12px
			border-right 1px solid #fff

			span
				margin 0 0 0 8px
			i
				color #fff
				margin 1px 6px 0 0
			//	float left
			// a
			// 	color #f1f1f1
			// 	font-size 11px
			// 	font-weight normal
			// 	text-decoration none
			// 	font-family $fonttext_f2
			// 	float left
			// 	margin-top 2px
			// 	&:hover
			// 		text-decoration underline

	.login
		font-family $fonttext_f2
		//margin-top 15px
		display inline-block
		//margin-right 0px
		float right
		margin-top 20px


		a
			text-decoration none
			color #29b34b
			font-size: 11px
			float left



			&:hover
				color #29b34b
			span
				float right
				display block


			i
				float right
				display none
			u
				float right
				display block

		.logout
			margin 0 0 0 10px
			color: #ccc
			font-size: 11px
			font-weight: normal
			float right
			a
				text-decoration none
				float none
				&:hover
					color #29B34B
			b
				display none
		.glyphicon-user
			vertical-align: middle !important;
			border-radius: 50%;
			padding 0px;
			font-size 22px
			color #29B34B
			margin-top 17px

	#logo
		display block
		text-align center
		margin-right -15px


		.visible_descktop
			display block
		.visible_mobile
			display none
		img
			display inline-block
			//margin-right -70px
			//width auto !important
			//height auto !important


	.menumyaccount
		font-family 'Arial'
		margin 33px 0 0 0
		display inline-block
		border-right 1px solid #ffffff
		width 100%
		text-align right
		a
			color #fff
			text-decoration none
			font-size 11px
			text-align left
			line-height 12px
			display inline-block
			.ico-user
				background url(../../images/common/sprite.png) no-repeat -8px -5px
				width 19px
				height 20px
				float left
				margin 2px 10px 0 0
			span
				float left
				width 40px
				margin 0 20px 0 0
		b
			border-left 1px solid #fff
			color #fff
			display inline-block
			font-size 11px
			font-weight normal
			line-height 14px
			padding-left 25px
			text-align left
			text-decoration none
			width 80px
			span
				display block
			b
				display none

	.menushopcart
		text-align right
		font-family $fonttext_f3
		font-size 22px
		font-weight normal
		//margin 17px 5px 0 0
		height 36px
		display block
		width 100%
		float right
		margin-top 16px
		margin-right 12px


		i
			float none
			margin-right 5px
			font-size 22px
			color #fff
			margin-top 2px

		a
			color #e3bb56
			margin-top 0px
			display block
			font-size 24px
			line-height 25px
			font-family $fonttext_f3
			letter-spacing -3px
			text-align center
			float right


		.ico-cart
			width 26px
			height 20px
			float left
			margin 3px 0px 0 0

	.header_sec_search
		display inline-block
		height auto
		width 100%
		font-family $fonttext1
		margin 18px 0 0 11px
		text-align right
		.search_wrapper
			width 350px
			background #fff
			z-index 3000
			position absolute
			right 0
			margin-top -7px
			margin-right -13px


			.search_box
				border-right 1px solid #29B34B
				float left
				height 36px
				width 91%
				.input_search
					width 86%
					right 0px
					height 25px
					border none
					padding-left 13px
					color $color_s
					border none
					font-weight 100
					border-radius 0px
					margin 3px 0 0 0
					font-size 16px
					line-height 18px
					float left
					text-align left
					font-family $fonttext_f1
				.button
					cursor pointer
					border none
					font-size 22px
					border-radius 0
					float right
					text-align center
					background transparent
					color $color_boton_primario
					padding 5px 13px 0 0
					&:hover
						//background: $color_t

			.close_popup
				width 9%
				color #29B34B
				font-size 25px
				cursor pointer
				display inline-block
				padding 0 8px 0 8px
				text-align center

		.t_search
			.fa-search
				// background url(../../images/common/search.jpg) no-repeat 0 0
				// background-size 90%
				font-size 22px
				color #fff
	.visible_search_mobile
		display none


	.menuhome
		display block
		border none
		.navbar-nav
			li
				padding-top 7px
				padding-bottom 5px
				a
					margin 0
		#nav_menu > li
			// padding 10px 15px
			// float left
			position inherit

		#nav_menu > li > a
			font-size 15px
			text-transform uppercase
			color #fff
			font-family $fonttext_f2
			text-decoration none
			background-color transparent

			&:hover
				background transparent
				color $color_brow
		.dropdown-menu
			padding-bottom 10px
			li
				padding 5px 0
				a
					&:hover
						background transparent
						color $color_gray_3






@media (min-width:992px) and (max-width:1199px)



	.dom-content-header
		.header_sec_search
			.search_wrapper
				//margin -6px 0 0 -195px
		.menushopcart
			margin 17px 10px 0 0

		.menuhome
			#nav_menu > li > a
				padding 10px 20px


@media (min-width:768px)

	.navbar
		.navbar-nav
			display inline-block
			float none
		.navbar-collapse
			text-align center



@media (min-width:768px) and (max-width:991px)



	.video-container
		height 290px
	.menu_header
		display none
	.dom-content-header
		min-height auto
		height auto
		margin 0 0 5px 0
		.top_slogan
			.client_pref
				margin-left 10px
		#logo
			//margin 5px 0 0 10px
			text-align center
			img
				display inline-block


		.header_sec_search
			margin 16px 0 0 0
			.search_wrapper
				width 260px
				margin-right 20px
				.search_box
					width 88%
					.input_search
						width 78%
				.close_popup
					width 11%
		.menuhome
			#nav_menu > li > a
				padding 10px 35px 10px 10px
		.menushopcart
			margin 15px 10px 0
		.login
			//margin-right -71px
			//margin-top -53px




@media (max-width:767px)

	.dom-content-header
		min-height auto
		height auto
		position relative
		#logo-main
			margin 0 auto
		.top_slogan
			padding 5px 0
			.client_pref
				margin-left 5px
				span
					display inline-block
				i
					font-size 16px
			.promotion
				font-size 11px
		.login
			width 100%
			a
				display inline-block
				margin-top -20px
				float none

				span
					display none !important
				i
					display block !important
				u
					display block
			.logout
				margin 0
				a
					text-decoration none
					float none
					font-size 18px
					padding-top 17px
					color #cc2929
					b
						display block

		#logo
			margin-left 0px


		.top_movil
			margin 0px 0 0 0
		.top_movil_search
			margin -40px 0 0
			position realtive
			z-index 100

		.top_movil_cart
			margin 13px 0 0
			padding 0px
			position absolute

		.header_sec_search
			margin 0 0 0 0
			width 100%
			.input_search
				width 80%
			.button
				width 14%
			.search_wrapper
				margin -7px 0 0 -190px
			/*MOBILE*/
			.ico-search
				width 40px
				height 40px
				border-radius 100%
				background $color_p
				color $color_s
				cursor pointer
				.glyphicon-search
					width 100%
					text-align center
					font-size 20px
					margin 8px 0 0 0
			.t_search
				.icon_t_search
					margin 0

		.navbar-default
			.navbar-toggle
				float left

				background #fff
				margin-top -7%
			.navbar-brand
				//display none

		.top_movil_search
			margin 16px 0 0
			position absolute
		.top_movil_cart
			margin 14px 0 0 0

		.menushopcart
			margin 0 0 0 50px
			float none
			font-size 25px
			display inline-block
			width 100%
			text-align right
			position absolute
			i
				float left
				margin 0 5px 0 0px
			a
				float left
				display inline-block
		.navbar
			min-height 0


		.menuhome
			margin 0px 0 0 0
			display block
			#nav_menu
				margin 0
				background #000
			#nav_menu > li
				margin 0px
				border-top 1px solid #777
			#nav_menu > li > a
				padding 0px
				// margin 0 0 5px 0
				color #fff
				padding 10px 0 5px 15px
				width 100%
				display block

		.dropdown
			.dropdown-menu
				background #000 !important
				margin-left 0 !important
		.dropdown .dropdown-menu li
			margin 5px 0 0 0px
			width 100%
		.dropdown .dropdown-menu li a
			color #fff !important
			padding 10px 0 5px 15px
			display block
			width 100%
		// .dropdown .dropdown-menu li a:hover
		// 	color #ffffff
		.dropdown .dropdown-menu strong

			font-weight normal
			text-transform uppercase
			// width 105%
		.dropdown .dropdown-menu strong a
			color #fff
			padding 10px 0
			width 100%
			display block


		.dropdown .dropdown-menu span
			display none


		/*BUTTON MENU - TOGGLE*/
		.navbar-toggle
			//margin-bottom 0px
			margin-right 0px
			margin-top 10px
			padding 10px 	7px
			background #fff
			border none
			color $color_s





		.navbar-default .navbar-toggle:hover, .navbar-default .navbar-toggle:focus
			border none
			background #fff
		.navbar-default .navbar-toggle
			//border 1px solid #000
		.navbar-default .navbar-toggle .icon-bar
			background #000
		.navbar-default .navbar-collapse, .navbar-default .navbar-form
			border none
			margin 0px 0 0 0

		/*.header_sec_search
			.input_search
				padding-top 5px
				line-height 20px*/





